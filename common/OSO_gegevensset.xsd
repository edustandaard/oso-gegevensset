      <xs:element name="verzuim" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Gegevens over aspecten die betrekking hebben op verzuim in de laatste 12 kalendermaanden</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="dagdelengeoorloofd" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">PO aantal dagdelen geoorloofd verzuim tijdens de afgelopen 12 maanden. Een dagdeel is ochtend of middag.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="dagdelenongeoorloofd" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">PO aantal dagdelen ongeoorloofd verzuim tijdens de afgelopen 12 maanden. Een dagdeel is ochtend of middag.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="urengeoorloofd" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">VO aantal uren geoorloofd verzuim tijdens de afgelopen 12 maanden.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="urenongeoorloofd" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">VO aantal uren ongeoorloofd verzuim.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="meerdan25percent" type="xs:boolean" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Is het meer dan 25% van de lestijd tijdens de afgelopen 12 maanden verzuimd?</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="frequentie" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Frequentie van het ongeoorloofd verzuim tijdens de afgelopen 12 maanden. Hierbij wordt iedere aparte registratie van verzuim
									geteld.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="contactleerplicht" type="xs:boolean" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Heeft contact met leerplichtambtenaar plaats gevonden?</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="rmcmelding" type="xs:boolean" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Is het verzuim gemeld bij RMC?</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="documentLijst" minOccurs="0">
              <xs:complexType>
                <xs:annotation>
                  <xs:documentation source="semantiek">Lijst van documenten met info over het verzuim. </xs:documentation>
                </xs:annotation>
                <xs:sequence>
                  <xs:element ref="document" maxOccurs="unbounded"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
