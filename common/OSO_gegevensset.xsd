  <!-- EINDTOETSRESULTAAT -->
  <xs:element name="eindtoetsresultaat" type="EindtoetsToetsType"/>
  <xs:complexType name="EindtoetsToetsType" final="extension">
    <xs:annotation>
      <xs:documentation source="semantiek">Details van het eindtoetsresultaat.</xs:documentation>
      <xs:documentation source="wijzigingen">20160120 dit blok en onderliggende elementen toegevoegd; dit blok is afgeleid van gelijknamige element uit REP (d.w.z. overbodige elementen en
				gegevenswaarden weggecommentarieerd )</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="toetssoort" type="EindtoetsToetssoortType"/>
      <xs:element name="toetsniveau" type="EindtoetsToetsniveauType"> </xs:element>
      <!--<xs:element name="toetstype" type="EindtoetsToetstypeType"/>-->
      <xs:element name="toetsomschrijving" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Omschrijving van de toets.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--<xs:element name="afn_awbnr" type="EindtoetsAfnAwbnrType"/>-->
      <xs:element name="datum_afname" type="xs:date">
        <xs:annotation>
          <xs:documentation source="semantiek">De datum waarop de eindtoets is afgenomen.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--<xs:element name="datum_vaststelling" type="xs:date" minOccurs="0"/>-->
      <!--<xs:element name="compleet_scoorbaar" type="EindtoetsCompleetScoorbaarType"/>-->
      <xs:element name="wettelijke_ontheffing" type="EindtoetsWettelijkeOntheffingType" minOccurs="0"/>
      <xs:element name="standaardscore" type="EindtoetsStandaardscoreType" minOccurs="0"/>
      <xs:element name="toets_advies" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Toetsadvies zoals tekstueel weergegeven op het leerlingrapport</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="EindtoetsToetsAdviesType">
            <xs:minLength value="0"/>
            <xs:maxLength value="1024"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <!--<xs:element name="toets_opg_aantal" type="EindtoetsOpgAantalType" minOccurs="0"/>-->
      <!--<xs:element name="toets_opg_goed" type="EindtoetsOpgGoedType" minOccurs="0"/>-->
      <!--<xs:element name="toets_opg_goed_omz" type="EindtoetsOpgGoedOmzType" minOccurs="0"/>-->
      <!--<xs:element name="toets_perc_land" type="EindtoetsPercType" minOccurs="0"/>-->
      <xs:element name="onderdelen" type="EindtoetsOnderdelenType">
        <xs:annotation>
          <xs:documentation source="semantiek">Blok met resultaten per onderdeel.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="EindtoetsToetssoortType">
    <xs:annotation>
      <xs:documentation source="semantiek">Soort eindtoets.</xs:documentation>
      <xs:documentation source="implementatie">Waarde volgens codelijst “42. Soort eindtoets” in “OSO codelijsten”, zoals bijvoobeeld “0011” (voor CET), “0012” (voor Route 8) en “0013” (voor
				IEP). Sinds eind 2016 zijn totaal zes eindtoetsen toegestaan.</xs:documentation>
      <xs:documentation source="wijzigingen">20161128 enumeration in restriction verwijderd; validatie van codelijst 42 in XSLT</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="EindtoetsToetsniveauType">
    <xs:annotation>
      <xs:documentation source="semantiek">Niveau van de eindtoets.</xs:documentation>
      <xs:documentation source="implementatie">Waarde volgens codelijst “43. Niveau eindtoets” in “OSO codelijsten”, zoals bijvoobeeld “B” (voor Basis) en “E” (voor
				Eenvoudig).</xs:documentation>
      <xs:documentation source="wijzigingen">20161128 enumeration in restriction verwijderd; validatie van codelijst 43 in XSLT</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="EindtoetsWettelijkeOntheffingType">
    <xs:annotation>
      <xs:documentation source="semantiek">Indicatie of leerling wettelijke ontheffing heeft voor de eindtoets basisonderwijs. </xs:documentation>
      <xs:documentation source="implementatie">Waarde volgens codelijst “44. Wettelijke ontheffing eindtoets” in “OSO codelijsten”, zoals J en N voor Ja resp. Nee. Defaultwaarde is “N”: geen
				ontheffing</xs:documentation>
      <xs:documentation source="wijzigingen">20161128 enumeration in restriction verwijderd; validatie van codelijst 44 in XSLT</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="EindtoetsStandaardscoreType">
    <xs:annotation>
      <xs:documentation source="semantiek">Toetsuitslag op de eindtoets als geheel in de vorm van de score.</xs:documentation>
      <xs:documentation source="implementatie">Sinds eind 2016 is het waardebereik 0 t/m 1000 om de scores van alle toegestande eindtoetsen te kunnen bevatten.</xs:documentation>
      <xs:documentation source="wijzigingen">20161128 restriction opgerekt van 0 t/m 1000 (was van 50 t/m 550)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="1000"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="EindtoetsToetsAdviesType">
    <xs:annotation>
      <xs:documentation source="semantiek">Toetsadvies zoals tekstueel weergegeven op het leerlingrapport.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="EindtoetsReferentieniveauType">
    <xs:annotation>
      <xs:documentation source="semantiek">Referentieniveau (Meijerink) op onderdeel of domein.</xs:documentation>
      <xs:documentation source="implementatie">Waarde volgens codelijst “45. Eindtoets referentieniveaus” in “OSO codelijsten”, zoals “01” t/m “04” voor “Lager dan 1F”, “1F”, “1S” resp.
				“2F”</xs:documentation>
      <xs:documentation source="wijzigingen">20161128 enumeration in restriction verwijderd; validatie van codelijst 45 in XSLT</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:complexType name="EindtoetsOnderdelenType" final="extension">
    <xs:sequence>
      <xs:element name="onderdeel" type="EindtoetsOnderdeelType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EindtoetsOnderdeelType" final="extension">
    <xs:annotation>
      <xs:documentation source="semantiek">Het resultaat van de afname van het onderdeel van de eindtoets.</xs:documentation>
      <xs:documentation source="wijzigingen">20160120 afgeleid van gelijknamige element uit REP (d.w.z. overbodige elementen en gegevenswaarden weggecommentarieerd )</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="onderdeelcode" type="EindtoetsOnderdeelcodeType"/>
      <xs:element name="onderdeelomschrijving" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Omschrijving van het onderdeel.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--<xs:element name="ond_opg_aantal" type="EindtoetsOpgAantalType"/>-->
      <!--<xs:element name="ond_opg_goed" type="EindtoetsOpgGoedType"/>-->
      <!--<xs:element name="ond_opg_goed_omz" type="EindtoetsOpgGoedOmzType" minOccurs="0"/>-->
      <!--<xs:element name="ond_score" type="EindtoetsScoreType" minOccurs="0"/>-->
      <!--<xs:element name="ond_perc_land" type="EindtoetsPercType" minOccurs="0"/>-->
      <xs:element name="ond_referentieniveau" type="EindtoetsReferentieniveauType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Referentieniveau (Meijerink) op onderdeel.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="domeinen" type="EindtoetsDomeinenType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="EindtoetsOnderdeelcodeType">
    <xs:annotation>
      <xs:documentation source="semantiek">De unieke code voor een onderdeel van de eindtoets.</xs:documentation>
      <xs:documentation source="implementatie">Waarde volgens codelijst “46. Eindtoets onderdeelcode” in “OSO codelijsten”, zoals 8000 (Taal) of 8001 (Rekenen). Let op, waardebereik beperkt tot
				onderdelen met referentieniveau(s).</xs:documentation>
      <xs:documentation source="wijzigingen">20161128 enumeration in restriction verwijderd; validatie van codelijst 46 in XSLT</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:complexType name="EindtoetsDomeinenType" final="extension">
    <xs:annotation>
      <xs:documentation source="semantiek">Blok met resultaten per domein.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="domein" type="EindtoetsDomeinType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="semantiek">Blok met resultaten van een domein.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EindtoetsDomeinType" final="extension">
    <xs:annotation>
      <xs:documentation source="semantiek">Het resultaat van de afname van het domein incl subdomeinen binnen een onderdeel van de eindtoets..</xs:documentation>
      <xs:documentation source="wijzigingen">20160120 afgeleid van gelijknamige element uit REP (d.w.z. overbodige elementen en gegevenswaarden weggecommentarieerd )</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="domeincode" type="EindtoetsDomeincodeType"/>
      <xs:element name="domeinomschrijving" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Omschrijving van het domein.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--<xs:element name="dom_opg_aantal" type="EindtoetsOpgAantalType"/>-->
      <!--<xs:element name="dom_opg_goed" type="EindtoetsOpgGoedType" minOccurs="0"/>-->
      <!--<xs:element name="dom_opg_goed_omz" type="EindtoetsOpgGoedOmzType" minOccurs="0"/>-->
      <!--<xs:element name="dom_score" type="EindtoetsScoreType" minOccurs="0"/>-->
      <xs:element name="dom_referentieniveau" type="EindtoetsReferentieniveauType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Referentieniveau (Meijerink) op domein.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--<xs:element name="subdomeinen" type="EindtoetsSubdomeinenType" minOccurs="0"/>-->
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="EindtoetsDomeincodeType">
    <xs:annotation>
      <xs:documentation source="semantiek">De unieke code voor een domein.</xs:documentation>
      <xs:documentation source="implementatie">Waarde volgens codelijst “47. Eindtoets domeincode” in “OSO codelijsten”, zoals 8050 (Lezen) of 8051 (Taalverzorging). Let op, waardebereik beperkt
				tot domeinen met referentieniveau.</xs:documentation>
      <xs:documentation source="wijzigingen">20161128 enumeration in restriction verwijderd; validatie van codelijst 47 in XSLT</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <!-- HANDELINGSPLAN -->
  <xs:element name="handelingsplan" type="handelingsplanType"/>
  <xs:complexType name="handelingsplanType">
    <xs:annotation>
      <xs:documentation source="semantiek">Details van een handelingsplan.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="startdatum" type="xs:date" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation source="semantiek">De startdatum van het handelingsplan.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="einddatum" type="xs:date" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation source="semantiek">De einddatum van het handelingsplan.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="isgerichtop" type="nonEmptyStringType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation source="semantiek">Aanduiding op welk domein, vak, vormingsgebied het handelingsplan is gericht.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="document" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation source="semantiek">Document met informatie en details over een handelingsplan.</xs:documentation>
          <xs:documentation source="implementatie">Dit blok "document" bevat 3 velden: bestandsnaam, omschrijving en bestandsinhoud. Het veld omschrijving bevat bijvoorbeeld
						"Handelingsplan".</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- HUIDIGESCHOOL -->
  <xs:element name="huidigeschool" type="huidigeschoolType"/>
  <xs:complexType name="huidigeschoolType">
    <xs:annotation>
      <xs:documentation source="semantiek">Schoolgegevens van de huidige school van de leerling in het dossier (inclusief leerlinggegevens).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="schoolType">
        <xs:sequence>
          <xs:element ref="leerling"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!-- INSTELLING -->
  <xs:element name="instelling" type="instellingType"/>
  <xs:complexType name="instellingType">
    <xs:sequence>
      <xs:element name="naam" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">De naam van de instelling/school.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="adreslijst" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Lijst van adressen van de instelling/school.</xs:documentation>
          <xs:documentation source="implementatie">Bedenk dat meer dan één adres wordt afgeraden omdat de ontvangende applicatie hierop veelal niet is berekend. En het is onzeker welk adres
						in dat geval wordt overgenomen. Ook adres van soort “gba” wordt afgeraden.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="adres" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="communicatielijst" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Lijst van communicatiegegevens (telefoonnummer of e-mailadres) van de instelling/school.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="communicatie" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- INZAGE -->
  <xs:element name="inzage" type="inzageType"/>
  <xs:complexType name="inzageType">
    <xs:annotation>
      <xs:documentation source="semantiek">Inzagegegevens over het dossier.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="voorlopigdossier" type="xs:boolean">
        <xs:annotation>
          <xs:documentation source="semantiek">Aanduiding of het een voorlopig dossier is. Het dossier kan in verschillende stadia (aanmelding, plaatsing en inschrijving) opgevraagd worden.
						De status is relevant voor de opvrager. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="inzage" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Hebben ouders/verzorgers inzage gehad? Met dit gegeven is voor de opvrager helder of de ouders inzage hebben gehad in het dossier. </xs:documentation>
          <xs:documentation source="wijzigingen">20131128 veld inzage optioneel geworden voor profiel 'Overdracht binnen Brin'</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="inzagedatum" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Met deze inzagedatum is voor de opvrager helder wanneer de ouders/verzorgers het dossier hebben ingezien.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="akkoord" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Gaan ouders/verzorgers (of leerling) akkoord met inhoud. Als de ouders/verzorgers het dossier hebben ingezien, is ook bekend of ze het al dan
						niet eens zijn met de inhoud ervan. Let op, dit veld betreft de indicatie of ouders/verzorgers het wel of niet eens zijn met de inhoud en niet of ze het eens zijn met de
						overdracht.</xs:documentation>
          <xs:documentation source="implementatie">Dit veld bevat waarde volgens codelijst “37. Akkoord”, zoals "Nee", "Ja" en "Onbekend".</xs:documentation>
          <xs:documentation source="wijzigingen">20140519 restriction verwijderd; validatie van codelijst 37 in XSLT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="redennietakkoord" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Als de ouders/verzorgers het niet eens zijn met de inhoud van dit dossier, is er hier ruimte voor een toelichting waarom zij het niet eens zijn
						met de inhoud.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="toestemming" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Hebben de ouders/verzorgers (of de leerling zelf) toestemming gegeven voor overdracht van het dossier. Als de leerling en/of ouders/verzorgers
						het dossier hebben ingezien, is ook bekend of ze het al dan niet hebben toegestemd in de overdracht.</xs:documentation>
          <xs:documentation source="implementatie">Waarde volgens codelijst “37. Akkoord”, zoals "Nee", "Ja" en "Onbekend". </xs:documentation>
          <xs:documentation source="wijzigingen">20150126 veld "toestemming" toegevoegd</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- LEERLING -->
  <xs:element name="leerling">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="leerlingType"/>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="leerlingType">
    <xs:sequence>
      <xs:element name="voornaam" type="nonEmptyStringType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="semantiek">De voornaam van de leerling.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="voorletters-1" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De voorletter of voorletters van de leerling. Gebruik wel punten maar geen spaties tussen de voorletters (b.v. “Chr.Th.” of
						“R.A.”.).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="voorvoegsel" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Tussenvoegsel (tussen voor- en achternaam) van de leerling, ook wel voorvoegsel genoemd.</xs:documentation>
          <xs:documentation source="implementatie">Waarde is bijvoorbeeld "van der"</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="achternaam" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">De achternaam van de leerling.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="roepnaam" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De roepnaam van de leerling.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice minOccurs="0">
        <xs:element name="geboortedatum" type="xs:date" minOccurs="0">
          <xs:annotation>
            <xs:documentation source="semantiek">De geboortedatum van de leerling.</xs:documentation>
            <xs:documentation source="implementatie">Formaat: EEJJ-MM-DD</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="geboortemaand" type="geboortemaandType" minOccurs="0">
          <xs:annotation>
            <xs:documentation source="semantiek">De anonieme geboortedatum van de leerling t.b.v. leeftijdbepaling.</xs:documentation>
            <xs:documentation source="wijzigingen">20160120 veld "geboortemaand" toegevoegd inclusief omvattende choice statement. </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="geslacht" type="geslachtType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Het geslacht van de leerling.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="adresgeheim" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Indicatie of het adres wel of niet geheim is.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice minOccurs="0">
        <xs:element name="adreslijst" minOccurs="0">
          <xs:annotation>
            <xs:documentation source="semantiek">De lijst met adressen van de leerling. Container met één of meer adressen conform element Adres.</xs:documentation>
            <xs:documentation source="implementatie">Bedenk dat meer dan één leerlingadres wordt afgeraden omdat de ontvangende applicatie hierop veelal niet is berekend. En het is onzeker
							welk adres in dat geval wordt overgenomen.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="adres" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="postcode4adres" type="nonEmptyStringType" minOccurs="0">
          <xs:annotation>
            <xs:documentation source="semantiek">De anonieme postcode van de leerling t.b.v. regiobepaling. Bevat alleen de vier cijfers van de postcode van het adres. Let op, gebruik van
							dit veld bij leerling alleen in plaats van het voorgaande veld “Adresgegevens leerling” (adreslijst).</xs:documentation>
            <xs:documentation source="implementatie">Waarde is alleen de 4 cijfers van Nederlandse postcode van bestaand adres. </xs:documentation>
            <xs:documentation source="wijzigingen">20160120 postcode4adres toegevoegd inclusief omvattende choice statement. </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="communicatielijst" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De lijst van communicatiegegevens van de leerling; container met 1 of meer elementen Communicatie.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="communicatie" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="gba" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De gegevens over de naam van de leerling volgens het GBA.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="gbavoornaam" type="nonEmptyStringType" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation source="semantiek">De voornaam of voornamen van de leerling volgens GBA.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="gbavoorvoegsel" type="nonEmptyStringType" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Voorvoegsel van de leerling volgens GBA.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="gbaachternaam" type="nonEmptyStringType">
              <xs:annotation>
                <xs:documentation source="semantiek">Achternaam van de leerling volgens GBA.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="gbageboortedatum">
              <xs:annotation>
                <xs:documentation source="semantiek">Geboortedatum van de leerling volgens GBA.</xs:documentation>
                <xs:documentation source="implementatie">Let op, dit veld heeft een andere datumnotatie dan alle andere datumvelden, n.l. “DD-MM-EEJJ”.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:pattern value="\d{2}-\d{2}-\d{4}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="adresbuitenland" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="semantiek">Buitenlands adres van de leerling.</xs:documentation>
          <xs:documentation source="implementatie">Tekstregels van minimale lengte 1 en maximale lengte 35 (per tekstregel).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="35"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="geboorteplaats" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De geboorteplaats van de leerling.</xs:documentation>
          <xs:documentation source="wijzigingen">20131128 veld geboorteplaats voor leerling toegevoegd</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="nationaliteit" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="semantiek">De GBA-code van de nationaliteit van de leerling, met attribuut voor omschrijving.</xs:documentation>
          <xs:documentation source="implementatie">gba code nationaliteit</xs:documentation>
          <xs:documentation source="wijzigingen">20131128 veld nationaliteit voor leerling toegevoegd</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attribute name="omschrijving" type="nonEmptyStringType" use="optional">
                <xs:annotation>
                  <xs:documentation source="semantiek">Omschrijving van de nationaliteit, de optionele uitbreiding van het voorgaande veld nationaliteit met de
										code.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:choice minOccurs="0">
        <xs:element name="onderwijsnummer" type="nonEmptyStringType">
          <xs:annotation>
            <xs:documentation source="semantiek">Onderwijsnummer van de leerling.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="bsn" type="nonEmptyStringType">
          <xs:annotation>
            <xs:documentation source="semantiek">Burgerservicenummer van de leerling.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="leerlingid" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Unieke code van de leerling in schooladministratie (LASkey).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="onderwijssinds" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Datum vanaf wanneer Nederlands onderwijs wordt gevolgd.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="startdatumhuidigonderwijs" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Aanvangsdatum van het huidige onderwijs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="datuminschrijving" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Datum van eerste schooldag.</xs:documentation>
          <xs:documentation source="wijzigingen">20111103 datuminschrijving is optioneel geworden, omdat het in de datadictionary alleen voor LAS verplicht is</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="datumuitschrijving" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Datum van laatste schooldag.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="teruggeplaatstnaarbrin" type="brincodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">BRIN-nummer van school waarnaar de leerling is teruggeplaatst vanuit een WEC-school.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="leerlinggewicht" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Actuele leerlinggewicht volgens de gewichtenregeling in het basisonderwijs.</xs:documentation>
          <xs:documentation source="implementatie">Waarde volgens codelijst “15. Gewicht”, zoals “0.00”, “0.30” en “1.20”.</xs:documentation>
          <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 15 in XSLT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="nnca" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Code voor indicatie NNCA (Niet Nederlandse Culturele Achtergrond).</xs:documentation>
          <xs:documentation source="implementatie">Waarde volgens codelijst “10. NNCA”, zoals “0” (=n.v.t.), “1” (=Nederlands) en “2” (Niet-Nederlands).</xs:documentation>
          <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 10 in XSLT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="tehuis" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">ndicatie of leerling wel of niet in tehuis woont. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="huisarts" type="persoonType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Persoonsgegevens van de huisarts.</xs:documentation>
          <xs:documentation source="wijzigingen">20190101 huisarts toegevoegd (i.p.v. artsenlijst)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="verzorgersaansprakelijk" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Indicatie of verzorgers/ouders aansprakelijk zijn voor de leerling.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="aansprakelijkeinstelling" type="instellingType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Gegevens van aansprakelijke instelling.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="overstapadvies" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation source="semantiek">Gegevensblok over het advies. </xs:documentation>
          <xs:documentation source="wijzigingen">20131128 gegevensblok overstapadvies is optioneel geworden, omdat alle deelelementen optioneel zijn</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="advies" type="voadviesduoType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">Het advies van de school m.b.t. leerling in VO (schooladvies).</xs:documentation>
                <xs:documentation source="wijzigingen">20111103 het type van het veld gewijzigd in een code</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="wensouders" type="voadviesduoType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">VO advies volgens ouders</xs:documentation>
                <xs:documentation source="wijzigingen">20111103 het type van het veld gewijzigd in een code</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="wensleerling" type="voadviesduoType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">VO advies volgens leerling</xs:documentation>
                <xs:documentation source="wijzigingen">20131128 veld wensleerling toegevoegd</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="herzienadvies" type="voadviesduoType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">Het herziene schooladvies, zoals na verwerking van de resultaten van de eindtoets basisonderwijs. Alleen indien afwijkend van
									schooladvies.</xs:documentation>
                <xs:documentation source="wijzigingen">20140509 veld herzienadvies toegevoegd</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="motivatie_herzienadvies" type="nonEmptyStringType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">De motivatie voor herziening van het schooladvies (b.v. “Resultaten van de eindtoets basisonderwijs” of
									“Foutherstel”.</xs:documentation>
                <xs:documentation source="implementatie">Alleen indien voorgaande veld "Herzien VO advies" aanwezig is.</xs:documentation>
                <xs:documentation source="wijzigingen">20150126 veld "motivatie_herzienadvies" toegevoegd</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="profiel" type="nonEmptyStringType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">Profiel dat als extra info kan worden toegevoegd.</xs:documentation>
                <xs:documentation source="implementatie">Veld heeft waarde uit codelijst “21. Profiel” in bestand “OSO codelijsten”, zoals “basis”, “plus”, “bespreek” en
									“disharmonisch”.</xs:documentation>
                <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 21 in XSLT</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="argumentatie" type="nonEmptyStringType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">Motivering of toelichting bij het VO advies, d.w.z. het schooladvies, het advies volgens ouders en/of het advies volgens
									leerling.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="contactnodig" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">Is contact met aanleverende school nodig?</xs:documentation>
                <xs:documentation source="wijzigingen">20131128 veld contactnodig optioneel geworden voor profiel 'Overdracht binnen Brin'</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="contactpersoon" type="nonEmptyStringType" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">De naam van de persoon bij de aanleverende school, die optreedt als contactpersoon voor deze leerling.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="contacttelefoon" type="nonEmptyStringType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">Telefoonnummer van deze contactpersoon.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="contactemail" type="nonEmptyStringType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">E-mailadres van deze contactpersoon.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="aandachtspunten" type="aandachtspuntenType" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation source="semantiek">Hiermee kan kort worden aangegeven of er aandacht nodig is bij de overdracht, bijvoorbeeld rond extra ondersteuningsbehoefte,
									multidisciplinaire begeleiding, ingrijpende gebeurtenissen, specifieke risico’s bij de overstap. Let op de privacy en dataminimalisatie!</xs:documentation>
                <xs:documentation source="wijzigingen">20190101 Dit veld Aandachtspunten toegevoegd ihkv Onderwijsopndersteuning</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="vanpeuterspeelzaal" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Indicatie of leerling afkomstig is van peuterspeelzaal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="naampeuterspeelzaal" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Naam van deze peuterspeelzaal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="voorschools" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Programma en deelname van voorschoolse educatie voor peuters (VVE-peuter).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="codevoorschoolsehistorie" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Code van het soort voorschool, die de leerling gevolgd heeft.</xs:documentation>
                <xs:documentation source="implementatie">Dit veld heeft waarde uit vocabulaire volgens codelijst “7. Voorschoolse historie”</xs:documentation>
                <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 7 in XSLT</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="codevoorschoolsprogramma" type="xs:integer">
              <xs:annotation>
                <xs:documentation source="semantiek">Code van het VVE programma dat het kind (leeftijd 2-4 jaar) gevolgd heeft op de voorschool.</xs:documentation>
                <xs:documentation source="implementatie">Dit veld heeft waarde uit vocabulaire volgens codelijst “9. VVE programma” in bestand “OSO codelijsten”</xs:documentation>
                <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 9 in XSLT</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="voorschoolsemaanden">
              <xs:annotation>
                <xs:documentation source="semantiek">Het aantal maanden dat de leerling heeft deelgenomen aan het voorschools programma.</xs:documentation>
                <xs:documentation source="implementatie">Dit veld heeft waarde uit bereik {1, …, 24}</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1"/>
                  <xs:maxInclusive value="24"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="codevoorschoolsetoeleider" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Code van de instantie, die gezin heeft verwezen naar voorschool.</xs:documentation>
                <xs:documentation source="implementatie">Waarde uit vocabulaire volgens codelijst “6. Voorschoolse toeleider” in bestand “OSO codelijsten”</xs:documentation>
                <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 6 in XSLT</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="codevoorschoolsedeelname" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Code van de intensiteit van de deelname aan het voorschools programma.</xs:documentation>
                <xs:documentation source="implementatie">Dit veld heeft waarde uit vocabulaire volgens codelijst “8. Voorschoolse deelname”</xs:documentation>
                <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 8 in XSLT</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="voorschoolseaanwezigheid" type="xs:integer" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">Het aantal dagdelen per week van deelname aan voorschools programma.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="vroegschools" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="semantiek">Programma en data van vroegschoolse educatie voor kleuters (VVE-kleuter).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="codevroegschoolsprogramma" type="xs:integer">
              <xs:annotation>
                <xs:documentation source="semantiek">Code van het VVE programma dat het kind (leeftijd 4-6 jaar) gevolgd heeft op de voorschool.</xs:documentation>
                <xs:documentation source="implementatie">Waarde volgens codelijst “9. VVE programma”, zoals veld “Code voorschools programma”</xs:documentation>
                <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 9 in XSLT</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="vroegschoolsdatumingang" type="xs:date">
              <xs:annotation>
                <xs:documentation source="semantiek">De startdatum van het VVE programma.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="vroegschoolseinddatum" type="xs:date" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="semantiek">De einddatum van het VVE programma.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="schoolloopbaanlijst" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Historie van scholen en schooljaren van de leerling in container element met 1 of meer elementen Schoolloopbaan.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="schoolloopbaan" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="onderwijsondersteuning" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation source="semantiek">Onderwijsondersteuningsgegevens over de leerling.</xs:documentation>
          <xs:documentation source="implementatie">Vanaf versie 2019.1 is de naam van dit gegevensblok gewijzigd in Onderwijsondersteuning en de inhoud ingrijpend
