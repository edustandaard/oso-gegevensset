<!-- 201904015 Update release van versie 2019.1, inclusief wijzigingen:
	 			bijlagedocument <document> in blok <tlv> gewijzigd in optioneel (was verplicht). 
   20190101 Eerste concept release van versie 2019.1, inclusief wijzigingen:
	 			versienummer van de namespace aangepast naar "2019" (i.p.v. "2018")
	 			uitbreiding met velden en blokken m.b.t. onderwijsondersteuning: 
	 			blok <huisarts> toegevoegd (i.p.v. artsenlijst), 
	 			veld <aandachtspunten> aan blok <overstapadvies> toegevoegd, 
	 			blok <onderwijsondersteuning> (i.p.v. zorg) ingrijpend veranderd, 
	 			blok SE functioneren verwijderd, 
	 			blok Handelingsplannen verplaatst. 
	 20180102 Eerste concept release van versie 2018.1, inclusief wijzigingen:
	 			versienummer van de namespace aangepast naar "2018" (i.p.v. "2017")
	 			uitbreiding met velden <overleden> en <overlijdensdatum> blok <verzorger> t.b.v. overleden ouders. 
	 20170308 Eerste officiele release van versie 2017.1, inclusief wijzigingen:
	 			versienummer van de namespace aangepast naar "2017" (i.p.v. "2016")
	 			uitbreiding met optioneel blok "Categorie uitlevering" t.b.v. selectief uitleveren. 
	 			alle elementen zijn voorzien van documentatie informatie uit het document van de afspraak, tevens herschikking van volgorde (zonder aanpassingen aan elementen)
     
     *** algemene indeling van de documentatie ***
	<xs:annotation>
		<xs:documentation source="semantiek">Korte beschrijvende aanduiding en/of definitie van de betekenis</xs:documentation>
		<xs:documentation source="implementatie">Aanwijzingen voor implementatie (of gebruik)</xs:documentation>
		<xs:documentation source="wijzigingen">Wijzigingshistorie</xs:documentation>
	</xs:annotation>
-->
<xs:schema xmlns="http://www.edustandaard.nl/oso_gegevensset/2019/dossier" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.edustandaard.nl/oso_gegevensset/2019/dossier" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <!-- DOSSIER -->
  <xs:element name="dossier" type="dossierType"/>
  <xs:complexType name="dossierType">
    <xs:annotation>
      <xs:documentation source="semantiek">Verzameling van leer- en begeleidingsgegevens van een leerling</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="metadata"/>
      <xs:element name="overdrachtsoort" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">Soort overdracht</xs:documentation>
          <xs:documentation source="implementatie">Waarde uit Codelijst 29 in bestand "OSO codelijsten", zoals "overstapdossier" of "overdrachtbinnenbrin"</xs:documentation>
          <xs:documentation source="wijzigingen">20140509 De restriction verwijderd; validatie van codelijst 29 in XSLT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="inzage" minOccurs="0"/>
      <xs:element ref="huidigeschool"/>
    </xs:sequence>
  </xs:complexType>
  <!-- ADRES -->
  <xs:element name="adres" type="adresType"/>
  <xs:complexType name="adresType">
    <xs:annotation>
      <xs:documentation source="semantiek">De gegevens van een adres, inclusief straat, huisnummer, postcode en plaats.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="soortadres" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">Soort adres</xs:documentation>
          <xs:documentation source="implementatie">Waarde volgens codelijst “19. Soort adres” in bestand OSO codelijsten, zoals “bezoek”, “post” en “gba”.</xs:documentation>
          <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 19 in XSLT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="straat">
        <xs:annotation>
          <xs:documentation source="semantiek">De straatnaam of de postbus (als het om een postadres gaat).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="nummer" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">Huisnummer</xs:documentation>
          <xs:documentation source="implementatie">Als er een waarde ingevuld is, moet deze voorkomen in combinatie met de postcode in de postcodetabel van TNT.</xs:documentation>
          <xs:documentation source="wijzigingen">Wijzigingshistorie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="toevoeging" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De huisnummertoevoeging wordt gebruikt als aanvulling op de straatnaam en het huisnummer, bijvoorbeeld als adres Reiger 32A is, wordt hier A
						ingevuld.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="aanduiding" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De huisnummeraanduiding wordt alleen gebruikt als aanvulling op de straatnaam en het huisnummer. Vaak wordt dit gebruikt bij
						woonboten.</xs:documentation>
          <xs:documentation source="implementatie">Waarde volgens codelijst “18. Huisnummer aanduiding” in bestand OSO codelijsten, zoals “TO” en “BY”.</xs:documentation>
          <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 18 in XSLT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="locatieomschrijving" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De locatieomschrijving wordt alleen - indien nodig - gebruikt als aanvulling op de straatnaam en het huisnummer. Als op het adres Reiger 32A
						een extra aanduiding nodig, is kan dat hier gebruikt worden, bijvoorbeeld: 3-hoog-achter..</xs:documentation>
          <xs:documentation source="implementatie">Waarde is bijvoorbeeld "3-hoog-achter"</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="35"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="postcode" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">De Nederlandse postcode (4 cijfers en 2 letters) of buitenlandse postcode (4 of 5 cijfers).</xs:documentation>
          <xs:documentation source="implementatie">Geen vast formaat</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="plaats" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">De woon-, bezoek- of correspondentieplaats.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="gemeente" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De gemeente waar de plaats in valt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="land" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De lgba code (volgens de landenlijst van de GBA) van het land waar de plaats in valt, met attribuut voor omschrijving.</xs:documentation>
          <xs:documentation source="implementatie">Waarde volgens de landenlijst van de GBA, zie ook OSO codelijsten.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="nonEmptyStringType">
              <xs:attribute name="omschrijving" type="nonEmptyStringType" use="optional">
                <xs:annotation>
                  <xs:documentation source="semantiek">Omschrijving van het woonland.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- ARTS -->
  <!--<xs:element name="arts" type="artsType"/>
	<xs:annotation>
		<xs:documentation source="semantiek">Arts</xs:documentation>
		<xs:documentation source="wijzigingen">20190101 arts verwijderd (huisarts toegevoegd) </xs:documentation>
	</xs:annotation>
	<xs:complexType name="artsType">
		<xs:complexContent>
			<xs:extension base="persoonType">
				<xs:sequence>
					<xs:element name="soort" type="nonEmptyStringType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>-->
  <!-- CIJFERLIJST -->
  <xs:element name="cijferlijst">
    <xs:annotation>
      <xs:documentation source="semantiek">Cijferlijst</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cijferlijstType"/>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="cijferlijstType">
    <xs:annotation>
      <xs:documentation source="semantiek">Cijferlijst</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cijferlijstTypelijst" type="cijferlijstTypelijstType">
        <xs:annotation>
          <xs:documentation source="semantiek">Details van de cijferlijst.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="afnamedatum" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Datum van cijferlijst.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="jaargroep" type="jaargroepType">
        <xs:annotation>
          <xs:documentation source="semantiek">Jaargroep van decijferlijst.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="periode" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Periode van de cijferlijst.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="cijferlijstTypelijstType">
    <xs:annotation>
      <xs:documentation source="semantiek">Details over naam en scores van de cijferlijst.</xs:documentation>
    </xs:annotation>
    <xs:sequence maxOccurs="unbounded">
      <xs:element name="naam" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">Naam van cijferlijst.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="onderdeel" type="onderdeelType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="semantiek">Onderdelen van cijferlijst per onderdeel</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="score" type="scoreType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="semantiek">Totaalscores van cijferlijst, als waarde of kwalificatie.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="onderdeelType">
    <xs:annotation>
      <xs:documentation source="semantiek">Details van het onderdeel (o.a. rapportcijfer) van cijferlijst.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="naam" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">Naam van het onderdeel van een cijferlijst, zoals “Rekenen” of "Taal".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="score" type="scoreType">
        <xs:annotation>
          <xs:documentation source="semantiek">Rapportcijfer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="afnamedatum" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Afnamedatum van het rapportcijfer</xs:documentation>
          <xs:documentation source="wijzigingen">20111103 toegevoegd o.b.v. datadefinitie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="jaargroep" type="jaargroepType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Jaargroep of leerjaar van het rapportcijfer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="periode" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Periode van het rapportcijfer.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="scoreType">
    <xs:annotation>
      <xs:documentation source="semantiek">Score van cijferlijst of onderdeel (rapportcijfer), als waarde of kwalificatie.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="waarde" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Waarde van score</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="kwalificatie" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Kwalificatie van score</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- COMMUNICATIE -->
  <xs:element name="communicatie" type="communicatieType"/>
  <xs:complexType name="communicatieType">
    <xs:annotation>
      <xs:documentation source="semantiek">De gegevens van een communicatiesoort, zoals telefoonnummer of e-mailadres.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="soort" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">Aanduiding van soort communicatie.</xs:documentation>
          <xs:documentation source="implementatie">Waarde volgens codelijst “4. Communicatie soort” in bestand “OSO codelijsten", zoals “telefoon” en “e-mail”.</xs:documentation>
          <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 4 in XSLT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!-- -->
      <xs:element name="aanduiding" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Aanduiding of het communicatiemiddel (nummer dan wel e-mailadres) zakelijk of privé wordt gebruikt.</xs:documentation>
          <xs:documentation source="implementatie">Waarde volgens codelijst “5. Communicatie aanduiding” in bestand “OSO codelijsten”, zoals “werk”, “prive” en “overig”.</xs:documentation>
          <xs:documentation source="wijzigingen">20140509 restriction verwijderd; validatie van codelijst 5 in XSLT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="nummer" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">Het telefoonnummer of e-mailadres.</xs:documentation>
          <xs:documentation source="implementatie">Een e-mailadres is minimaal 4 en maximaal 256 tekens. Het teken ‘@’ is verplicht en het deel ervoor maximaal 64 tekens (cijfers, letters of
						teken uit !#$% *+=?^_`{|}~.-). Het deel erna (domeinnaam) alleen cijfers, letters, punten en minnen. Deze voorschriften zijn conform DUO. Aanbeveling: om voor een
						telefoonnummer alleen cijfers te gebruiken (voor een NL-nummer exact 10 cijfers en voor buitenlands nummer meer cijfers).</xs:documentation>
          <xs:documentation source="wijzigingen">20160201 De restrictie over het formaat van een e-mailadres toegevoegd</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="geheim" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Indicatie of nummer wel of niet een geheim nummer is.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="noodnummer" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">Indicatie of dit nummer een noodnummer is. Scholen hebben doorgaans bij één of twee telefoonnummers geregistreerd dat het in noodgevallen te
						bereiken is.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- DOCUMENT -->
  <xs:element name="document" type="documentType"/>
  <xs:complexType name="documentType">
    <xs:annotation>
      <xs:documentation source="semantiek">Bijlagedocument is een generiek blok.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="bestandsnaam" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">De bestandsnaam van het document dat in het dossier als bijlage wordt meegezonden. </xs:documentation>
          <xs:documentation source="implementatie">Omdat gebruikers van de betrokken applicaties deze bijlagedocumenten moeten kunnen inzien wordt een lijst met toegelaten bestandstypes
						voorgeschreven. Alleen bestanden van het type PDF, JPG, PNG, DOC, DOCX, ODF, XLS, XLSX, TXT, PPT en/of PPTX zijn als bijlagedocument toegestaan.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="omschrijving" type="nonEmptyStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="semantiek">De omschrijving van de aard van het document.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bestandsinhoud" type="nonEmptyStringType">
        <xs:annotation>
          <xs:documentation source="semantiek">De inhoud van het bestand. </xs:documentation>
          <xs:documentation source="implementatie">Bijlagedocument als Base64 gecodeerd ZIP-bestand</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
